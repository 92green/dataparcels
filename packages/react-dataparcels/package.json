{
  "name": "react-dataparcels",
  "version": "1.0.2",
  "description": "A library for editing data structures that works really well with React.",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "Damien Clarke",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueflag/dataparcels.git"
  },
  "files": [
    "lib",
    "Action.js",
    "asyncChange.js",
    "asChildNodes.js",
    "asNode.js",
    "asRaw.js",
    "asyncChange.js",
    "asyncValue.js",
    "cancel.js",
    "ChangeRequest.js",
    "deleted.js",
    "ParcelBoundary.js",
    "ParcelBoundaryHoc.js",
    "ParcelHoc.js",
    "ParcelNode.js",
    "asChildNodes.js",
    "useParcelBuffer.js",
    "useParcelForm.js",
    "useParcelState.js",
    "translate.js",
    "validation.js"
  ],
  "bugs": {
    "url": "https://github.com/blueflag/dataparcels/issues"
  },
  "private": false,
  "scripts": {
    "build": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --ignore '**/__test__/*.js' --ignore '**/__mocks__/*.js'",
    "build-all": "yarn build",
    "flow": "blueflag-test flow",
    "flow-coverage": "blueflag-test flow-coverage",
    "lint": "blueflag-test lint",
    "size": "size-limit",
    "test": "yarn build && yarn jest --maxWorkers=1",
    "test-all": "yarn build && yarn jest --maxWorkers=1 && yarn flow && yarn lint && yarn flow-coverage && yarn size",
    "watch": "yarn run build -w"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "dataparcels": "^1.0.2",
    "is-promise": "^2.1.0",
    "unmutable": "^0.47.1",
    "use-debounce": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "babel-preset-blueflag": "^1.0.0",
    "blueflag-test": "^0.22.0",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.9.0-alpha",
    "react-dom": "^16.9.0-alpha",
    "react-hooks-testing-library": "^0.5.0",
    "size-limit": "1.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "resolutions": {
    "**/react-test-renderer": "16.9.0-alpha.0"
  }
}
